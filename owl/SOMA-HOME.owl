    <!-- http://www.ease-crc.org/ont/SOMA-HOME.owl#CerealBox -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA-HOME.owl#CerealBox">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA-HOME.owl#Box"/>
