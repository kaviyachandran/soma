    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/SOMA-HOME.owl#TemperingByHeating -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA-HOME.owl#TemperingByHeating">
